{
  "name": "ts-retrofit2",
  "version": "2.2.0",
  "description": "A declarative and axios based retrofit implementation for JavaScript and TypeScript. (forked from https://github.com/nullcc/ts-retrofit)",
  "repository": "https://github.com/npwork/ts-retrofit2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "retrofit",
    "ts",
    "js",
    "axios"
  ],
  "bugs": "https://github.com/npwork/ts-retrofit2/issues",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "tsc",
    "format": "prettier --config .prettierrc '{src,test}/**/*.ts' --write",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "make-badges": "istanbul-badges-readme",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn run lint"
  },
  "author": "NP",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && yarn lint && yarn make-badges && git add README.md"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.0",
    "form-data": "^3.0.0",
    "qs": "^6.9.4",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.22",
    "@types/multer": "1.4.5",
    "@types/qs": "6.9.6",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "body-parser": "1.19.0",
    "eslint": "7.24.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-unused-imports": "1.1.1",
    "express": "4.17.1",
    "husky": "4.3.8",
    "istanbul-badges-readme": "1.2.1",
    "jest": "26.6.3",
    "multer": "1.4.2",
    "prettier": "2.2.1",
    "ts-jest": "26.5.4",
    "typescript": "4.2.4"
  }
}
